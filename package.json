{
  "name": "puppeteer-test",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "lint": "eslint --fix 'src/**/*.{ts,tsx,js,jsx}'",
    "prod": "npm run build && node ./dist/src/main.js",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:prod": "npm run build && node ./dist/src/main.js",
    "test": "ts-node -r tsconfig-paths/register src/utils/test-ip.ts",
    "build": "rm -rf ./dist/ && tsc -p tsconfig.json",
    "ts": "ts-node -r tsconfig-paths/register ./logs/test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SunshowerC/puppeteer-test.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SunshowerC/puppeteer-test/issues"
  },
  "homepage": "https://github.com/SunshowerC/puppeteer-test#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.6.8",
    "@types/puppeteer": "^1.12.4",
    "@types/request": "^2.48.2",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "chalk": "^2.4.2",
    "eslint": "^5.15.0",
    "eslint-config-elegant": "^1.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-typescript": "^1.1.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^2.2.0",
    "lint-staged": "^13.1.0",
    "nodemon": "^1.19.0",
    "prettier": "1.14.3",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.7.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "jsdom": "^15.1.1",
    "lodash": "^4.17.15",
    "mysql": "^2.17.1",
    "puppeteer": "^1.18.1",
    "request": "^2.88.0",
    "typeorm": "^0.2.18",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  }
}
